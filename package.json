    "autoprefixer": "^10.4.15",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "langchain": "^0.0.163",
    "postcss": "^8.4.28",
    "rc-tween-one": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "tailwindcss": "^3.3.3",
    "uuid": "^9.0.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "~16.2.1",
